{{ form_start(form) }}
    <div class="row">
        <div class="col">
            {{ form_row(form.titre) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(form.description) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(form.image) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(form.categorie) }}
        </div>
    </div>
    <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
